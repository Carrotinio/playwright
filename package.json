{
  "name": "playwright-tutorial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test_chrome": "npx playwright test --project=chromium",
    "test_mobile": "npx playwright test --project='Mobile Safari'",
    "test_ui": "npx playwright test --ui",
    "test_headed": "npx playwright test --headed",
    "test_report": "npx playwright test && npx playwright show-report",
    "test_failed": "npx playwright test --last-failed",
    "test_list": "npx playwright test --list",
    "generate_allure": "allure generate --clean ./allure-results -o ./allure-report",
    "open_allure": "allure generate --clean ./allure-results -o ./allure-report & allure open ./allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "allure-playwright": "^3.4.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "ajv": "^8.17.1",
    "playwright": "^1.55.0"
  }
}
